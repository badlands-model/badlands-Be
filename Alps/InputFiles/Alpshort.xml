<?xml version="1.0" encoding="UTF-8"?>
<badlands xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<grid>
<demfile>../Data/initial_topo.csv</demfile>
<boundary>fixed</boundary>
<resfactor>1</resfactor>
<nopit>1</nopit>
</grid>
<time>
<start>0.</start>
<end>10000.</end>
<display>100.</display>
</time>
<sea>
<position>0.</position>
</sea>
<precipitation>
<climates>1</climates>
<rain>
<rstart>0.</rstart>
<rend>100000.</rend>
<rval>0.5</rval>
</rain>
</precipitation>
<sp_law>
<dep>1</dep>
<slp_cr>0.005</slp_cr>
<perc_dep>0.5</perc_dep>
<fillmax>50.</fillmax>
<m>0.5</m>
<n>1.0</n>
<diffnb>5</diffnb>
<diffprop>0.2</diffprop>
<deepbasin>-2300.</deepbasin>
<dens_crit>0</dens_crit>
<a_rating>1.e-3</a_rating>
<b_rating>1.6</b_rating>
<erodibility>4.5e-6</erodibility>
</sp_law>
<cosmonuclides>
<quartz_map>../Data/quartz_map.csv</quartz_map>
<denudation_rate>2.e-4</denudation_rate>
<shield_factor>1.</shield_factor>
<latitude>40.</latitude>
</cosmonuclides>    
<creep>
<caerial>0.001</caerial>
<cmarine>0.025</cmarine>
<criver>0.025</criver>
</creep>        
<outfolder>Alpsmodel_short</outfolder>
</badlands>