</ice_cover>
<creep>
<caerial>0.001</caerial>
<cmarine>0.025</cmarine>
<criver>0.025</criver>
</creep>
<flexure>
<ftime>10000.0</ftime>
<dmantle>3300</dmantle>
<dsediment>2500</dsediment>
<youngMod>65E9</youngMod>
<elasticH>20000.</elasticH>
<boundary_W>0Slope0Shear</boundary_W>
<boundary_E>0Slope0Shear</boundary_E>
<boundary_S>0Slope0Shear</boundary_S>
