<?xml version="1.0" encoding="UTF-8"?>
<badlands xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<grid>
<demfile>../Data/initial_topo.csv</demfile>
<boundary>fixed</boundary>
<resfactor>1</resfactor>
<nopit>1</nopit>
</grid>
<time>
<start>0.</start>
<end>500000.</end>
<display>1000.</display>
</time>
<sea>
<position>0.</position>
<curve>../Data/sealevel100ka.txt</curve>
</sea>
<precipitation>
<climates>2</climates>
<rain>
<rstart>0.</rstart>
<rend>80000.</rend>
<rval>0.3</rval>
</rain>
 <rain>
<rstart>80000.</rstart>
<rend>2000000.</rend>
<rval>0.7</rval>
</rain>
</precipitation>
<sp_law>
<dep>1</dep>
<slp_cr>0.005</slp_cr>
